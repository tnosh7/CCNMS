<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="ranking">
	<select id="selectListRankingList" resultType="UserDTO">
		SELECT 
			*
		FROM 
			USER
		ORDER BY 
			LIKE_POINT DESC
		LIMIT 5
	</select>

</mapper>